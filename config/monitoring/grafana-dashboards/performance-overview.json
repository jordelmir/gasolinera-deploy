{
  "dashboard": {
    "id": null,
    "title": "Gasolinera JSM - Performance Overview",
    "tags": ["gasolinera-jsm", "performance", "overview"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 30,
    "version": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ]
    },
    "templating": {
      "list": [
        {
          "name": "service",
          "type": "query",
          "query": "label_values(up{job=~\".*-service\"}, job)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true,
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          }
        },
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(up{job=~\"$service\"}, instance)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true,
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          }
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Service Status Overview",
        "type": "stat",
        "gridPos": { "h": 4, "w": 24, "x": 0, "y": 0 },
        "targets": [
          {
            "expr": "up{job=~\"$service\"}",
            "legendFormat": "{{job}} - {{instance}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "thresholds" },
            "thresholds": {
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "green", "value": 1 }
              ]
            },
            "mappings": [
              { "options": { "0": { "text": "DOWN" } }, "type": "value" },
              { "options": { "1": { "text": "UP" } }, "type": "value" }
            ]
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 2,
        "title": "Request Rate (RPS)",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
        "targets": [
          {
            "expr": "sum(rate(http_server_requests_total{job=~\"$service\", instance=~\"$instance\"}[5m])) by (job)",
            "legendFormat": "{{job}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "custom": {
              "axisLabel": "Requests/sec",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": { "legend": false, "tooltip": false, "vis": false },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": { "type": "linear" },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": { "group": "A", "mode": "none" },
              "thresholdsStyle": { "mode": "off" }
            },
            "unit": "reqps"
          }
        },
        "options": {
          "legend": {
            "calcs": [],
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": { "mode": "single", "sort": "none" }
        }
      },
      {
        "id": 3,
        "title": "Response Time (95th Percentile)",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{job=~\"$service\", instance=~\"$instance\"}[5m])) by (job, le))",
            "legendFormat": "{{job}} - 95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(http_server_requests_seconds_bucket{job=~\"$service\", instance=~\"$instance\"}[5m])) by (job, le))",
            "legendFormat": "{{job}} - 50th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "custom": {
              "axisLabel": "Response Time",
              "axisPlacement": "auto",
              "drawStyle": "line",
              "fillOpacity": 10,
              "lineWidth": 1,
              "pointSize": 5,
              "showPoints": "never",
              "stacking": { "group": "A", "mode": "none" }
            },
            "unit": "s",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 0.2 },
                { "color": "red", "value": 0.5 }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
        "targets": [
          {
            "expr": "sum(rate(http_server_requests_total{job=~\"$service\", instance=~\"$instance\", status=~\"5..\"}[5m])) by (job) / sum(rate(http_server_requests_total{job=~\"$service\", instance=~\"$instance\"}[5m])) by (job)",
            "legendFormat": "{{job}} - Error Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "custom": {
              "axisLabel": "Error Rate",
              "axisPlacement": "auto",
              "drawStyle": "line",
              "fillOpacity": 10,
              "lineWidth": 2,
              "pointSize": 5,
              "showPoints": "never"
            },
            "unit": "percentunit",
            "max": 1,
            "min": 0,
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 0.01 },
                { "color": "red", "value": 0.05 }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "JVM Memory Usage",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
        "targets": [
          {
            "expr": "jvm_memory_used_bytes{job=~\"$service\", instance=~\"$instance\", area=\"heap\"} / jvm_memory_max_bytes{job=~\"$service\", instance=~\"$instance\", area=\"heap\"}",
            "legendFormat": "{{job}} - {{instance}} - Heap Usage"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "custom": {
              "axisLabel": "Memory Usage",
              "axisPlacement": "auto",
              "drawStyle": "line",
              "fillOpacity": 10,
              "lineWidth": 1,
              "pointSize": 5,
              "showPoints": "never"
            },
            "unit": "percentunit",
            "max": 1,
            "min": 0,
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 0.7 },
                { "color": "red", "value": 0.9 }
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "CPU Usage",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total{job=~\"$service\", instance=~\"$instance\"}[5m])",
            "legendFormat": "{{job}} - {{instance}} - CPU Usage"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "custom": {
              "axisLabel": "CPU Usage",
              "axisPlacement": "auto",
              "drawStyle": "line",
              "fillOpacity": 10,
              "lineWidth": 1,
              "pointSize": 5,
              "showPoints": "never"
            },
            "unit": "percentunit",
            "max": 1,
            "min": 0,
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 0.7 },
                { "color": "red", "value": 0.9 }
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Database Connection Pool Usage",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
        "targets": [
          {
            "expr": "hikaricp_connections_active{job=~\"$service\", instance=~\"$instance\"} / hikaricp_connections_max{job=~\"$service\", instance=~\"$instance\"}",
            "legendFormat": "{{job}} - {{instance}} - Connection Pool Usage"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "custom": {
              "axisLabel": "Pool Usage",
              "axisPlacement": "auto",
              "drawStyle": "line",
              "fillOpacity": 10,
              "lineWidth": 1,
              "pointSize": 5,
              "showPoints": "never"
            },
            "unit": "percentunit",
            "max": 1,
            "min": 0,
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 0.7 },
                { "color": "red", "value": 0.9 }
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Garbage Collection Time",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 28 },
        "targets": [
          {
            "expr": "rate(jvm_gc_collection_seconds_sum{job=~\"$service\", instance=~\"$instance\"}[5m])",
            "legendFormat": "{{job}} - {{instance}} - {{gc}} GC Time"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "custom": {
              "axisLabel": "GC Time per Second",
              "axisPlacement": "auto",
              "drawStyle": "line",
              "fillOpacity": 10,
              "lineWidth": 1,
              "pointSize": 5,
              "showPoints": "never"
            },
            "unit": "s",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 0.05 },
                { "color": "red", "value": 0.1 }
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Thread Count",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 28 },
        "targets": [
          {
            "expr": "jvm_threads_current{job=~\"$service\", instance=~\"$instance\"}",
            "legendFormat": "{{job}} - {{instance}} - Current Threads"
          },
          {
            "expr": "jvm_threads_peak{job=~\"$service\", instance=~\"$instance\"}",
            "legendFormat": "{{job}} - {{instance}} - Peak Threads"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "custom": {
              "axisLabel": "Thread Count",
              "axisPlacement": "auto",
              "drawStyle": "line",
              "fillOpacity": 10,
              "lineWidth": 1,
              "pointSize": 5,
              "showPoints": "never"
            },
            "unit": "short"
          }
        }
      }
    ]
  }
}
