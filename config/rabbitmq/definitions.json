{
  "rabbit_version": "3.12.0",
  "rabbitmq_version": "3.12.0",
  "product_name": "RabbitMQ",
  "product_version": "3.12.0",
  "users": [
    {
      "name": "gasolinera_user",
      "password_hash": "gasolinera_password",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator",
      "limits": {}
    }
  ],
  "vhosts": [
    {
      "name": "gasolinera_vhost"
    }
  ],
  "permissions": [
    {
      "user": "gasolinera_user",
      "vhost": "gasolinera_vhost",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "internal_cluster_id",
      "value": "rabbitmq-cluster-id-gasolinera-jsm"
    }
  ],
  "policies": [
    {
      "vhost": "gasolinera_vhost",
      "name": "ha-all",
      "pattern": ".*",
      "apply-to": "all",
      "definition": {
        "ha-mode": "all",
        "ha-sync-mode": "automatic"
      },
      "priority": 0
    },
    {
      "vhost": "gasolinera_vhost",
      "name": "ttl-policy",
      "pattern": ".*\\.ttl",
      "apply-to": "queues",
      "definition": {
        "message-ttl": 3600000,
        "expires": 7200000
      },
      "priority": 1
    }
  ],
  "queues": [
    {
      "name": "auth.events.queue",
      "vhost": "gasolinera_vhost",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 10000
      }
    },
    {
      "name": "coupon.validated.queue",
      "vhost": "gasolinera_vhost",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 10000
      }
    },
    {
      "name": "coupon.redeemed.queue",
      "vhost": "gasolinera_vhost",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 10000
      }
    },
    {
      "name": "redemption.completed.queue",
      "vhost": "gasolinera_vhost",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 10000
      }
    },
    {
      "name": "redemption.voided.queue",
      "vhost": "gasolinera_vhost",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 10000
      }
    },
    {
      "name": "raffle.tickets.generated.queue",
      "vhost": "gasolinera_vhost",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 10000
      }
    },
    {
      "name": "ad.engagement.completed.queue",
      "vhost": "gasolinera_vhost",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 10000
      }
    },
    {
      "name": "ad.tickets.multiplied.queue",
      "vhost": "gasolinera_vhost",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 10000
      }
    },
    {
      "name": "audit.general.queue",
      "vhost": "gasolinera_vhost",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-max-length": 50000
      }
    }
  ],
  "exchanges": [
    {
      "name": "gasolinera.events",
      "vhost": "gasolinera_vhost",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "gasolinera.audit",
      "vhost": "gasolinera_vhost",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "gasolinera.dlx",
      "vhost": "gasolinera_vhost",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "gasolinera.events",
      "vhost": "gasolinera_vhost",
      "destination": "coupon.validated.queue",
      "destination_type": "queue",
      "routing_key": "coupon.validated",
      "arguments": {}
    },
    {
      "source": "gasolinera.events",
      "vhost": "gasolinera_vhost",
      "destination": "coupon.redeemed.queue",
      "destination_type": "queue",
      "routing_key": "coupon.redeemed",
      "arguments": {}
    },
    {
      "source": "gasolinera.events",
      "vhost": "gasolinera_vhost",
      "destination": "redemption.completed.queue",
      "destination_type": "queue",
      "routing_key": "redemption.completed",
      "arguments": {}
    },
    {
      "source": "gasolinera.events",
      "vhost": "gasolinera_vhost",
      "destination": "redemption.voided.queue",
      "destination_type": "queue",
      "routing_key": "redemption.voided",
      "arguments": {}
    },
    {
      "source": "gasolinera.events",
      "vhost": "gasolinera_vhost",
      "destination": "raffle.tickets.generated.queue",
      "destination_type": "queue",
      "routing_key": "raffle.tickets.generated",
      "arguments": {}
    },
    {
      "source": "gasolinera.events",
      "vhost": "gasolinera_vhost",
      "destination": "ad.engagement.completed.queue",
      "destination_type": "queue",
      "routing_key": "ad.engagement.completed",
      "arguments": {}
    },
    {
      "source": "gasolinera.events",
      "vhost": "gasolinera_vhost",
      "destination": "ad.tickets.multiplied.queue",
      "destination_type": "queue",
      "routing_key": "ad.tickets.multiplied",
      "arguments": {}
    },
    {
      "source": "gasolinera.audit",
      "vhost": "gasolinera_vhost",
      "destination": "audit.general.queue",
      "destination_type": "queue",
      "routing_key": "audit.#",
      "arguments": {}
    }
  ]
}